<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MusicActivity"
            android:label="All Songs" />
        <activity
            android:name=".PlayerActivity"
            android:label="Player" />
        <activity
            android:name=".ArtistActivity"
            android:label="Artists"/>
        <activity
            android:name=".AlexActivity"
            android:label="Alex Schulz"/>
        <activity
            android:name=".ArminActivity"
            android:label="Armin"/>
        <activity
            android:name=".AviciiActivity"
            android:label="Avicii"/>
        <activity
            android:name=".JasonActivity"
            android:label="Jason Derulo"/>
        <activity
            android:name=".KygoActivity"
            android:label="Kygo"/>
        <activity
            android:name=".MartinActivity"
            android:label="Martin Garrix">
        </activity>
    </application>

</manifest>